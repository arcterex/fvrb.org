  <h1>Club Library</h1>
  <div id="content">
    <% @members.each do |member| %>
    <div class="user">
      <h3 class="book-owner"><%=h member.name %></h3>
      <ul class="books-owned">
        <% member.books.each do |book| %>
          <li><%= book.title %></li>
        <% end %>
      </ul>  
      <div class="clear"></div>
    </div><!-- /.user -->
    <% end %>
    <div class="clear"></div>    
  </div><!-- /#content -->